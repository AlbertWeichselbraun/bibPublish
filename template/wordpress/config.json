{"output_order": ["article",
                  "incollection",
                  "inproceedings",
                  "book",
                  "unpublished",
                  "phdthesis",
                  "mastersthesis"],
 "string_replacements": {"--": "-",
                         "\\&": "&amp",
			 "\n": " ",
			 "{": "",
		         "}": "",
                         "\\_": "_"},

 "attribute_expansions": {"author": "f'{format_author(author)}'",
	                  "booktitle": "f' {booktitle}'",
                          "editor": "f', Ed. {editor}'",
                          "number": "f'({number}):'",
                          "volume": "f' {volume}'",
                          "doi": "f'doi:{doi}'",
                          "address": "f', {address}'",
			  "journal": "f'{journal}'",
			  "pages": "f'{pages}'",
			  "note": "f'{note}'",
			  "title": "f'{title}'",
			  "year": "f'{year}'",
			  "ID": "f'{ID}'",
			  "school": "f'{school}'",
			  "coins": "",
			  "_title": "f'''{f'<a class=\"title\" title=\"{title}\" href=\"{eprint}\">{title}</a>' if 'eprint' in locals() else title}'''",
			  "_bibpublish": ""
                        },
 "links": {
            "abstract_url": "<a class=\"abstract\" title=\"Abstract\" href=\"{abstract_url}\">[Abstract]</a>",
            "url": "<a class=\"related\" title=\"{title}\" href=\"{url}\">[URL]</a>",
            "eprint": "<a class=\"download\" title=\"{title}\" href=\"{eprint}\">[PDF]</a>",
            "bibtex": "<a class=\"bib\" title=\"Citation\" href=\"{bibtex}\">[BIB]</a>",
            "title": "<a class=\"title\" title=\"{title}\" href=\"{eprint}\">{title}</a>",
            "doi": "<a class=\"doi\" title=\"DOI\" href=\"http://dx.doi.org/{doi}\">doi:{doi}</a>"
           }
}
